@{
    ViewBag.Title = "My Lists";
}
@model MyListsVM

<h3 class="display-5">My Lists</h3>

<ul class ="list-group list-group-numbered">
    @{
        for (int i = 0; i < Model.ListOfLists.Length; i++)
        {
            <partial name="_ListItem" model=Model.ListOfLists[i]/>
        }
    }
</ul>